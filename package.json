  "bugs": {
    "url": "https://github.com/Volllon/react-todo-app/issues"
  },
  "homepage": "https://github.com/Volllon/react-todo-app#readme",
  "devDependencies": {
    "concurrently": "^5.1.0"
  },
  "dependencies": {}
}
